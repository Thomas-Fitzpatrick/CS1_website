        <!--        <a href="/sigcse2019/results/new" target="_blank"><button class="btn btn-lg btn-primary btn-block">Create New Syllabus Entry</button></a>-->
            <!--</form>-->
        <!--</div>-->
        
        <p class="spacer"></p>
        <p class="show-heading">Download current collection as JSON:</p>
        <a href="/sigcse2019/csv"><button type = "button" class = "btn btn-primary">Download</button></a>
        <p class="spacer"></p>
        
        <p class="show-heading">New submissions:</p>
        
        <table class="table table-hover">
            <thead>
                <tr>
                    <th scope="col">Country</th>
                    <th scope="col">URL</th>
                    <th scope="col">Code</th>
                    <th scope="col">Title</th>
                    <th scope="col">Institution</th>
                    <th scope="col">Email</th>
                </tr>                                                   
            </thead>
            <tbody>
                <% submissions.forEach(function(submission){ %>
                        <tr>
                            <td><%= submission.country %></td>
                            <td><%= submission.url %></td>
                            <td><%= submission.code %></td>
                            <td><%= submission.title %></td>
                            <td><%= submission.institution %></td>
                            <td><%= submission.email %></td>
                        </tr>
                <% }) %>
            </tbody>
        </table>
    </div>

<% include ../partials/footer %>